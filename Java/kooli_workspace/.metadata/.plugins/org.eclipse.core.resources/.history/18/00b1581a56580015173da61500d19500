package seeeiolepraktikum2;

import lib.TextIO;

public class ylesanne3 {

	public static void main(String[] args){
		int inimesed, grupisuurus, gruppe, jaak;
		
		System.out.println("Kui palju inimesi on?");
		inimesed = TextIO.getInt();
				
		System.out.println("Kui palju inimesi on grupis?");
		grupisuurus = TextIO.getInt();
		
		System.out.println("Kokku saab moodustada " + inimesed/grupisuurus +" gruppi ja inimesi jääb üle " +inimesed%grupisuurus);
		
	}
}
