package seeeiolepraktikum2;

import lib.TextIO;

public class ylesanne3 {

	public static void main(String[] args){
		int people, grupisuurus;
		
		System.out.println("Kui palju inimesi on?");
		people = TextIO.getInt();
				
		System.out.println("Kui palju inimesi on grupis?");
		grupisuurus = TextIO.getInt();
		
		System.out.println("Kokku saab moodustada " + people/grupisuurus +" gruppi ja inimesi jääb üle " +people%grupisuurus);
		
	}
}
