package praktikum2;

import lib.TextIO;

public class ylesanne3 {

	public static void main(String[] args){
		double inimesed, grupisuurus, gruppe, jaak;
		
		System.out.println("Kui palju inimesi on?");
		inimesed = TextIO.getlnDouble();
		
		System.out.println("Kui palju inimesi on grupis?");
		grupisuurus = TextIO.getlnDouble();
		
		gruppe = inimesed / grupisuurus;
		jaak = inimesed % grupisuurus;
		
		System.out.println("Kokku saab moodustada" + gruppe +"gruppe ja inimesi jääb üle " +jaak);
		
	}
}
